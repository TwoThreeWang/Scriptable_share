// 今天上班吗小组件

let no = 'data:image/png;base64,'

let yes = 'data:image/png;base64,'

const data_json = await fetchdata()

const du = await fetchdu()

let widget = await createWidget()
if (!config.runsInWidget) {
  await widget.presentMedium()
}
Script.setWidget(widget)
Script.complete()

async function createWidget() {
  let w = new ListWidget()

  var imgURL = no
  if(data_json.data.work_tag=='周末' || data_json.data.work_tag=='休假'){
    var imgURL = yes
  }

  let imgReq = new Request(imgURL)
  let img = await imgReq.loadImage()
  w.backgroundImage = img

  w.addSpacer()

  const oneLine = w.addText(` “今天是 ${data_json.data.jieri}” `)
  oneLine.font = Font.boldSystemFont(18)
  oneLine.centerAlignText()
  oneLine.textColor = Color.black()
  
  w.addText('')

  const twoLine = w.addText(` “${du.msg}” `)
  twoLine.font = Font.boldSystemFont(18)
  twoLine.centerAlignText()
  twoLine.textColor = Color.black()

  return w
}

async function fetchdata() {
  const url = "https://api.error.work/api/util/calendar"
  let req = new Request(url)
  let json = await req.loadJSON()
  return json
}

async function fetchdu() {
  const url = "https://api.error.work/api/util/dutang"
  let req = new Request(url)
  let json = await req.loadJSON()
  return json
}
